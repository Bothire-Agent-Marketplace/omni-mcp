{
  "mcpServers": {
    "linear": {
      "command": "node",
      "args": [
        "/Users/vince/Projects/omni/servers/linear-mcp-server/dist/index.js"
      ],
      "env": {
        "LINEAR_API_KEY": "lin_api_aaNrNFE97uyankUxjxmzD7CMk4ZYvpXBRj87CCPz"
      }
    },
    "filesystem": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--network",
        "mcp-network",
        "-v",
        "/Users/vince/Projects/omni/data/files:/projects/files",
        "-v",
        "/Users/vince/Projects/omni/data/uploads:/projects/uploads",
        "mcp/filesystem:latest",
        "/projects/files",
        "/projects/uploads"
      ]
    },
    "database-toolbox": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--network",
        "mcp-network",
        "-v",
        "/Users/vince/Projects/omni/compose/configs/database-tools.yaml:/config/tools.yaml:ro",
        "us-central1-docker.pkg.dev/database-toolbox/toolbox/toolbox:0.7.0",
        "./toolbox",
        "--tools-file",
        "/config/tools.yaml",
        "--stdio"
      ]
    }
  }
}
